{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "anhskohbo/no-captcha": "^3.0",
        "arcanedev/robots": "~2.0",
        "bepsvpt/secure-headers": "^6.3",
        "cartalyst/stripe-laravel": "6.0.*",
        "components/jquery": "^3.3",
        "davejamesmiller/laravel-breadcrumbs": "3.x",
        "doctrine/dbal": "^2.7",
        "guzzlehttp/guzzle": "^6.5",
        "intervention/image": "^2.4",
        "jenssegers/date": "^3.4",
        "laravel/framework": "5.4.*",
        "laravel/installer": "1.3.1",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.4.*",
        "mguinea/laravel-robots": "^2.0",
        "nesbot/carbon": "1.22.*",
        "propaganistas/laravel-phone": "^4.2",
        "skagarwal/google-places-api": "^1.4",
        "spatie/laravel-sitemap": "^5.8",
        "stripe/stripe-php": "^6.28",
        "tymon/jwt-auth": "^0.5.12"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^1.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
